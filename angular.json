{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "fedha": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/fedha",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/template/css/main.css",
              "src/assets/template/css/util.css",
              "src/assets/template/vendor/animate/animate.css",
              "src/assets/template/vendor/animsition/css/animsition.css",
              "src/assets/template/vendor/css-hamburgers/hamburgers.css",
              "src/assets/template/vendor/daterangepicker/daterangepicker.css",
              "src/assets/template/vendor/lightbox2/css/lightbox.css",
              "src/assets/template/vendor/magnificPopup/magnific-popup.css",
              "src/assets/template/vendor/perfect-scrollbar/perfect-scrollbar.css",
              "src/assets/template/vendor/select2/select2.css",
              "src/assets/template/vendor/slick/slick-theme.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "src/styles.scss",
              "node_modules/animate.css/animate.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "src/assets/template/vendor/animsition/js/animsition.js",
              "src/assets/template/vendor/countdowntime/countdowntime.js",
              "src/assets/template/vendor/daterangepicker/daterangepicker.js",
              "src/assets/template/vendor/daterangepicker/moment.js",
              "src/assets/template/vendor/isotope/isotope.pkgd.min.js",
              "src/assets/template/vendor/lightbox2/js/lightbox.js",
              "src/assets/template/vendor/MagnificPopup/jquery.magnific-popup.js",
              "src/assets/template/vendor/parallax100/parallax100.js",
              "src/assets/template/vendor/perfect-scrollbar/perfect-scrollbar.min.js",
              "src/assets/template/vendor/select2/select2.js",
              "src/assets/template/vendor/slick/slick.min.js",
              "src/assets/template/vendor/sweetalert/sweetalert.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fedha:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fedha:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fedha:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fedha:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fedha:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "fedha"
}